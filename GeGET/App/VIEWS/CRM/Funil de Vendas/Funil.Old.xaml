                                                        <TextBlock x:Name="txtCliente" HorizontalAlignment="Stretch" Margin="67,35,56,0" TextTrimming="CharacterEllipsis" Text="{Binding Fantasia}" VerticalAlignment="Top" Grid.Column="1" FontFamily="{DynamicResource Novecento-Light}" FontSize="12" Foreground="DimGray" Height="14"/>
                                                        <TextBlock HorizontalAlignment="Left" Margin="10,52,0,0" TextTrimming="CharacterEllipsis" Text="Descrição:" VerticalAlignment="Top" Grid.Column="1" FontFamily="{DynamicResource Novecento-Book}" FontSize="12" Foreground="DarkGray" Height="14"/>
                                                        <TextBlock x:Name="txtDescricao" HorizontalAlignment="Stretch" Margin="87,52,56,0" TextTrimming="CharacterEllipsis" Text="{Binding Descricao}" VerticalAlignment="Top" Grid.Column="1" FontFamily="{DynamicResource Novecento-Light}" FontSize="12" Foreground="DimGray" Height="14"/>
                                                        <TextBlock HorizontalAlignment="Left" Margin="10,69,0,0" TextTrimming="CharacterEllipsis" Text="Cidade:" VerticalAlignment="Top" Grid.Column="1" FontFamily="{DynamicResource Novecento-Book}" FontSize="12" Foreground="DarkGray" Height="14" />
                                                        <TextBlock x:Name="txtCidade" HorizontalAlignment="Stretch" Margin="64,69,56,0" TextTrimming="CharacterEllipsis" Text="{Binding Cidade}" VerticalAlignment="Top" Grid.Column="1" FontFamily="{DynamicResource Novecento-Light}" FontSize="12" Foreground="DimGray" Height="14"/>
                                                        <TextBlock HorizontalAlignment="Left" Margin="10,86,0,0" TextTrimming="CharacterEllipsis" Text="Prazo:" VerticalAlignment="Top" Grid.Column="1" FontFamily="{DynamicResource Novecento-Book}" FontSize="12" Foreground="DarkGray" Height="14"/>
                                                        <TextBlock x:Name="txtPrazo" HorizontalAlignment="Stretch" Margin="58,86,56,0" TextTrimming="CharacterEllipsis" Text="{Binding Prazo}" VerticalAlignment="Top" Grid.Column="1" FontFamily="{DynamicResource Novecento-Light}" FontSize="12" Foreground="DimGray" Height="14"/>
                                                        <Grid Grid.Column="1">
                                                            <DockPanel >
                                                                <materialDesign:PopupBox VerticalAlignment="Top" HorizontalAlignment="Right" StaysOpen="False" Margin="0,15,0,0" Foreground="#7A2932" x:Name="PopupBox">
                                                                    <StackPanel Width="150">
                                                                        <Button x:Name="btnEditar" Content="Editar"/>
                                                                        <Separator/>
                                                                        <Button x:Name="btnNegocios" Content="Negócios"/>
                                                                    </StackPanel>
                                                                </materialDesign:PopupBox>
                                                            </DockPanel>
                                                        </Grid>
                                                        <Grid>
                                                            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                                                                <materialDesign:PackIcon Kind="Cards" Height="50" Width="50" Foreground="#7A2932" />
                                                            </StackPanel>

                                                        </Grid>
                                                    </Grid>
                                                </StackPanel>
                                                <!--Template-->
                                            </DataTemplate>
                                        </ListBox.ItemTemplate>
                                    </ListBox>
